{
  "akka": {
    "log-config-on-start": false,
    "log-dead-letters": 10,
    "log-dead-letters-during-shutdown": {},
    "log-dead-letters-suspend-duration": {
      "minutes": 5
    },
    "extensions": [],
    "actor": {
      "provider": "local",
      "guardian-supervisor-strategy": "akka.actor.DefaultSupervisorStrategy",
      "creation-timeout": {
        "seconds": 20
      },
      "mailbox": {
        "default-mailbox": {
          "mailbox-capacity": 1000,
          "mailbox-push-timeout-time": {
            "seconds": 10
          },
          "stash-capacity": {}
        }
      },
      "debug": {
        "receive": false,
        "auto-receive": false,
        "lifecycle": false,
        "fsm": false,
        "event-stream": false,
        "unhandled": false,
        "router-misconfiguration": false
      }
    },
    "coordinated-shutdown": {
      "default-phase-timeout": {
        "seconds": 5
      },
      "terminate-actor-system": true,
      "phases": {
        "before-service-unbind": {
          "depends_on": []
        },
        "service-unbind": {
          "depends_on": [
            "before-service-unbind"
          ]
        },
        "service-requests-done": {
          "depends_on": [
            "service-unbind"
          ]
        },
        "service-stop": {
          "depends_on": [
            "service-requests-done"
          ]
        },
        "before-cluster-shutdown": {
          "depends_on": [
            "service-stop"
          ]
        },
        "cluster-sharding-shutdown-region": {
          "depends_on": [
            "before-cluster-shutdown"
          ]
        },
        "cluster-leave": {
          "depends_on": [
            "cluster-sharding-shutdown-region"
          ]
        },
        "cluster-exiting": {
          "timeout": {
            "seconds": 10
          },
          "depends_on": [
            "cluster-leave"
          ]
        },
        "cluster-exiting-done": {
          "depends_on": [
            "cluster-exiting"
          ]
        },
        "cluster-shutdown": {
          "timeout": {
            "seconds": 10
          },
          "depends_on": [
            "cluster-exiting-done"
          ]
        },
        "before-actor-system-terminate": {
          "depends_on": [
            "cluster-shutdown"
          ]
        },
        "actor-system-terminate": {
          "tiemout": {
            "seconds": 10
          },
          "depends_on": [
            "before-actor-system-terminate"
          ]
        }
      }
    },
    "circuit-breaker": {
      "default": {
        "max-failures": 10,
        "call-timeout": {
          "seconds": 10
        },
        "reset-timeout": {
          "seconds": 15
        },
        "max-reset-timeout": {
          "days": 36500
        },
        "exponential-backoff": 1.0,
        "random-factor": 0.0,
        "exception-allowlist": []
      }
    }
  }
}