{
  "akka": {
    "log_config_on_start": false,
    "log_dead_letters": true,
    "log_dead_letters_during_shutdown": null,
    "log_dead_letters_suspend_duration": {
      "minutes": 5
    },
    "extensions": [],
    "actor": {
      "guardian_supervisor_strategy": "akka.actor.DefaultSupervisorStrategy",
      "creation_timeout": {
        "seconds": 20
      },
      "mailbox": {
        "default_mailbox": {
          "mailbox_capacity": 1000,
          "mailbox_push_timeout_time": {
            "seconds": 10
          },
          "stash_capacity": null,
          "throughput": 5
        }
      },
      "debug": {
        "receive": false,
        "auto_receive": false,
        "lifecycle": false,
        "fsm": false,
        "event_stream": false,
        "unhandled": false,
        "router_misconfiguration": false
      }
    },
    "coordinated_shutdown": {
      "default_phase_timeout": {
        "seconds": 5
      },
      "terminate_actor_system": true,
      "phases": {
        "before_service_unbind": {
          "depends_on": []
        },
        "service_unbind": {
          "depends_on": [
            "before_service_unbind"
          ]
        },
        "service_requests_done": {
          "depends_on": [
            "service_unbind"
          ]
        },
        "service_stop": {
          "depends_on": [
            "service_requests_done"
          ]
        },
        "before_cluster_shutdown": {
          "depends_on": [
            "service_stop"
          ]
        },
        "cluster_sharding_shutdown_region": {
          "depends_on": [
            "before_cluster_shutdown"
          ]
        },
        "cluster_leave": {
          "depends_on": [
            "cluster_sharding_shutdown_region"
          ]
        },
        "cluster_exiting": {
          "timeout": {
            "seconds": 10
          },
          "depends_on": [
            "cluster_leave"
          ]
        },
        "cluster_exiting_done": {
          "depends_on": [
            "cluster_exiting"
          ]
        },
        "cluster_shutdown": {
          "timeout": {
            "seconds": 10
          },
          "depends_on": [
            "cluster_exiting_done"
          ]
        },
        "before_actor_system_terminate": {
          "depends_on": [
            "cluster_shutdown"
          ]
        },
        "actor_system_terminate": {
          "tiemout": {
            "seconds": 10
          },
          "depends_on": [
            "before_actor_system_terminate"
          ]
        }
      }
    },
    "circuit_breaker": {
      "default": {
        "max_failures": 10,
        "call_timeout": {
          "seconds": 10
        },
        "reset_timeout": {
          "seconds": 15
        },
        "max_reset_timeout": {
          "days": 36500
        },
        "exponential_backoff": 1.0,
        "random_factor": 0.0,
        "exception_allowlist": []
      }
    }
  }
}